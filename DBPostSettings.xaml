<UserControl x:Class="DBPost.DBPostSettings"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:local="clr-namespace:DBPost"
			 mc:Ignorable="d"
			 d:DesignHeight="300"
			 d:DesignWidth="300"
             d:DataContext="{d:DesignInstance local:DBPost}">
    <StackPanel Margin="12">
        <CheckBox Content="艦これ統計データベースへデータを送信する" IsChecked="{Binding IsSendDb}"/> 
        <TextBlock>アクセスキー</TextBlock>
        <TextBox Text="{Binding DbAccessKey}"/>
    </StackPanel>
</UserControl>
